bsp;&nbsp;<span class=\"sh_keyword\">case<\/span> <span class=\"sh_number\">4<\/span><span class=\"sh_symbol\">:<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_function\">display<\/span><span class=\"sh_symbol\">(<\/span>d<span class=\"sh_symbol\">,<\/span>ht<span class=\"sh_symbol\">,<\/span>empty<span class=\"sh_symbol\">);<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_keyword\">break<\/span><span class=\"sh_symbol\">;<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_keyword\">case<\/span> <span class=\"sh_number\">5<\/span><span class=\"sh_symbol\">:<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_function\">exit<\/span><span class=\"sh_symbol\">(<\/span><span class=\"sh_number\">0<\/span><span class=\"sh_symbol\">);<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_cbracket\">}<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_function\">getch<\/span><span class=\"sh_symbol\">();<\/span><\/li><li>&nbsp;&nbsp;&nbsp;&nbsp;<span class=\"sh_cbracket\">}<\/span><\/li><li><span class=\"sh_cbracket\">}<\/span><\/li><\/ul><\/pre><pre class=\"snippet-textonly sh_sourceCode\">#include&lt;stdio.h&gt;\r\n#include&lt;conio.h&gt;\r\n#include&lt;process.h&gt;\r\nint hsearch(int key,int d,int *ht,int *empty)\r\n{\r\n  int i=key%(d);\r\n  int j=i;\r\n  int c=0;\r\n  do\r\n  {\r\n\tif(empty[j]||(*(ht+j)==key))\r\n\t\treturn j;\r\n\tc++;\r\n\tj=(i+c)%(d);\r\n  }while(j!=i);\r\n  return 0;\r\n}\r\n\r\nint search(int key,int d,int *ht,int *empty)\r\n{\r\n\tint b=hsearch(key,d,ht,empty);\r\n\tif(empty[b])\r\n\t\treturn -1;\r\n\telse\r\n\t\treturn b;\r\n}\r\nvoid  insert(int key,int d,int *ht,int *empty)\r\n{\r\n\tint b=hsearch(key,d,ht,empty);\r\n\tif(empty[b])\r\n\t{\r\n\t\tempty[b]=0;\r\n\t\t*(ht+b)=key;\r\n\t\tprintf(\"Element is inserted\\n\");\r\n\r\n\t}\r\n}\r\nvoid delete1(int  key,int d,int *ht,int* empty)\r\n{\r\n\tint b=hsearch(key,d,ht,empty);\r\n\t*(ht+b)=0;\r\n\tempty[b]=1;\r\n\tprintf(\"Element is delted\\n\");\r\n}\r\nvoid display(int d,int *ht,int *empty)\r\n{\r\n\tint i;\r\n\tprintf(\"Hash table elements are\\n\");\r\n\tfor(i=0;i&lt;d;i++)\r\n\t{\r\n\t\tif(empty[i])\r\n\t\t     printf(\"      0\");\r\n\t\telse\r\n\t\t     printf(\"%5d\",*(ht+i));\r\n\t}\r\n\tprintf(\"\\n\");\r\n}\r\nvoid main()\r\n{\r\n\tint choice=1;\r\n\tint key;\r\n\tint d,i;\r\n\tint *empty,*ht;\r\n\t\/\/clrscr();\r\n\tprintf(\"Enter the hash table size:\");\r\n\tscanf(\"%d\",&amp;d);\r\n\tht=(int*)malloc(d*sizeof(int));\r\n\tempty=(int*)malloc(d*sizeof(int));\r\n\tfor(i=0;i&lt;d;i++)\r\n\t  empty[i]=1;\r\n\twhile(1)\r\n\t{\r\n\t\tprintf(\"\\n\");\r\n\t\tprintf(\"\\n LINEAR PROBING\");\r\n\t\tprintf(\"\\n1.insert into hash table\");\r\n\t\tprintf(\"\\n2.delete from hash table\");\r\n\t\tprintf(\"\\n3.search the hash table\");\r\n\t\tprintf(\"\\n4.display the hash table\");\r\n\t\tprintf(\"\\n5.Exit\");\r\n\t\tprintf(\"Enter your choice:\");\r\n\t\tscanf(\"%d\",&amp;choice);\r\n\t\tswitch(choice)\r\n\t\t{\r\n\t\t\tcase 1:\r\n\t\t\t\t\tprintf(\"Enter the element:\");\r\n\t\t\t\t\tscanf(\"%d\",&amp;key);\r\n\t\t\t\t\tinsert(key,d,ht,empty);\r\n\t\t\t\t\tbreak;\r\n\t\t\tcase 2:\r\n\t\t\t\t\tprintf(\"Enter to remove from hash table:\");\r\n\t\t\t\t\tscanf(\"%d\",&amp;key);\r\n\t\t\t\t\tdelete1(key,d,ht,empty);\r\n\t\t\t\t\tbreak;\r\n\t\t\tcase 3:\r\n\t\t\t\t\tprintf(\"Enter the search element:\");\r\n\t\t\t\t\tscanf(\"%d\",&amp;key);\r\n\t\t\t\t\tif(search(key,d,ht,empty)==0)\r\n\t\t\t\t\t\tprintf(\"Not found\\n\");\r\n\t\t\t\t\telse\r\n\t\t\t\t\t\tprintf(\"Element found at index %d\",hsearch(key,d,ht,empty));\r\n\t\t\t\t\tbreak;\r\n\t\t\tcase 4:\r\n\t\t\t\t\tdisplay(d,ht,empty);\r\n\t\t\t\t\tbreak;\r\n\t\t\tcase 5:\r\n\t\t\t\t\texit(0);\r\n\t\t}\r\n\t\tgetch();\r\n\t}\r\n}\r\n<\/pre><\/div><\/div>","page_type":"text","route":"page\/text","slider_content":"posts","list_content":"posts","slider":null,"taxonomy":null,"gallery_tag":null,"sitepage_id":null,"detail":"default","sharing":null,"map_name":null,"map_address":null},"icon":null,"small_text":null},{"title":"List Page","page_id":"4","page_data":{"text_html":null,"page_type":"list","route":"page\/list","slider_content":"posts","list_content":"posts","slider":null,"taxonomy":null,"gallery_tag":null,"sitepage_id":null,"detail":"default","sharing":null,"map_name":null,"map_address":null},"icon":null,"small_text":null}]}}